# Ignore bundler config.
/rails-template/.bundle

# Ignore the default SQLite database.
/rails-template/db/*.sqlite3
/rails-template/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/rails-template/log/*.log
/rails-template/tmp

# Ignore application configuration
/rails-template/config/application.yml

/react-template/node_modules/
